package com.TheMasterMindGroup.Loops;

public class ForLoopsSum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int sum = 0;
		
		for(int i =1; i <= 100; i++) {
			
			sum = sum + i;
			
			
			
		}
		
		System.out.println("The sum is : " + sum);
		
	}

}
